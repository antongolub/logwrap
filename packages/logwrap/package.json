{
  "name": "@qiwi/logwrap",
  "version": "1.3.0",
  "private": false,
  "description": "Logwrap facade",
  "main": "dist/es5/index.js",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "dist/"
  ],
  "scripts": {
    "build_es6": "flow-remove-types src/ --out-dir dist/es6/",
    "build_es5": "BABEL_ENV=production babel src --out-dir dist/es5/",
    "build": "rm -rf dist && npm run build_es6 && npm run build_es5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qiwi/logwrap.git"
  },
  "keywords": [
    "logwrap",
    "logbind",
    "logpipe",
    "logwrap-core"
  ],
  "author": "Anton Golub <a.golub@qiwi.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qiwi/logwrap/issues"
  },
  "homepage": "https://github.com/qiwi/logwrap#readme",
  "dependencies": {
    "@qiwi/logwrap-core": "^1.0.0",
    "@qiwi/logwrap-mdc": "^1.0.0"
  },
  "devDependencies": {
    "lodash": "^4.17.11"
  }
}
